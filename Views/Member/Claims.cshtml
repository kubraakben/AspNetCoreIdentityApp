@{
    ViewData["Title"] = "Claims";
    Layout = "~/Areas/Admin/Views/Shared/_Layout.cshtml";
}
@model List<ClaimViewModel>
<h1>Claim Listesi</h1>

<table class="table table-striped">
	<thead>
		<tr>
			<th>Issuer</th>
			<th>Type</th>
			<th>Value</th>
			
		</tr>
	</thead>
	<tbody>
		@foreach (var item in Model)
		{
			<tr>
				<td>@item.Issuer</td>
				<td>@item.Type</td>
				<td>@item.Value</td>
			</tr>

		}
	</tbody>
</table>

